{
  "name": "trip-destination-viz",
  "version": "1.2.1",
  "description": "A visualization of daily trip destinations in the Edmonton Central Metropolitan Area in 2065",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "pretest": "webpack --mode production --config webpack.test.config.js",
    "test": "mocha test/test.build.js",
    "posttest": "rimraf test/test.build.js",
    "buildDev": "rimraf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "dev": "npm run buildDev && npm start",
    "buildProd": "rimraf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "prod": "npm run buildProd && npm start",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaslorincz/trip-destination-viz.git"
  },
  "author": "Thomas Lorincz",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/thomaslorincz/trip-destination-viz/issues"
  },
  "homepage": "https://github.com/thomaslorincz/trip-destination-viz#readme",
  "dependencies": {
    "eventemitter3": "^4.0.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "mapbox-gl": "^1.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/express": "^4.17.0",
    "chai": "^4.2.0",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-google": "^0.13.0",
    "eslint-loader": "^2.2.1",
    "esprima": "^4.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "mocha": "^6.1.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
}
