{
  "name": "trip-destination-viz",
  "version": "1.1.0",
  "description": "A visualization of daily trip destinations in the Edmonton Central Metropolitan Area in 2065",
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ie_mob 11"
  ],
  "scripts": {
    "buildDev": "rimraf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "dev": "npm run buildDev && npm start",
    "buildProd": "rimraf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "prod": "npm run buildProd && npm start",
    "start": "node ./dist/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thomaslorincz/trip-destination-viz.git"
  },
  "author": "Thomas Lorincz",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/thomaslorincz/trip-destination-viz/issues"
  },
  "homepage": "https://github.com/thomaslorincz/trip-destination-viz#readme",
  "dependencies": {
    "core-js": "^3.1.3",
    "express": "^4.17.1",
    "heroku-ssl-redirect": "0.0.4",
    "mapbox-gl": "^1.0.0",
    "regenerator-runtime": "^0.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-google": "^0.13.0",
    "eslint-loader": "^2.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.3",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2",
    "workbox-webpack-plugin": "^4.3.1"
  }
}
